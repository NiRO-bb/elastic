{
  "analysis": {
    "analyzer": {
      "custom_analyzer": {
        "tokenizer": "standard",
        "char_filter": [
          "remove_dot"
        ]
      }
    },
    "char_filter": {
      "remove_dot": {
        "type": "mapping",
        "mappings": [
          ". => \\u0020"
        ]
      }
    }
  }
}
